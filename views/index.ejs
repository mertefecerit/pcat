<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 p-0">
          <img class="img-fluid" src="https://via.placeholder.com/1920x300" alt="placeholder-image">
        </div>
      </div>
    <div class="row">
        <% images.forEach(image => { %>
            <div class="col-lg-3">
                <div class="p-2 shadow my-2">
                    <div class="card">
                        <img src="/uploads/<%= image.image %>" class="card-img-top" alt="...">
                        <div class="card-body">
                          <h5 class="card-title"><%= image.title %></h5>
                          <p class="card-text"><%= image.desc %></p>
                          <a href="#" class="btn btn-primary">Go somewhere</a>
                        </div>
                    </div>
                </div>
            </div>
        <% }) %>
    </div>
</div>